var arraySign = function(nums) {
    var sum=1;
    for(let i=0;i<nums.length;i++){
        sum=sum*nums[i];
      
    }
      if(sum>0){
            return 1
        }
        else if(sum<0){
            return -1
        }
        else{
            return 0
        }
};



//https://leetcode.com/problems/sign-of-the-product-of-an-array/?envType=study-plan&id=programming-skills-i
